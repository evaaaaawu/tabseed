{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Choose the Prometheus data source used by this dashboard",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "title": "TabSeed - Core",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "API RPS by route",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (route, method) (rate(http_requests_total[5m]))",
          "legendFormat": "{{route}} {{method}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "API Error Rate",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (route) (rate(http_requests_total{status=~\"5..|4..\"}[5m])) / sum by (route) (rate(http_requests_total[5m]))",
          "legendFormat": "{{route}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "API p95 latency",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, route, method) (rate(http_request_duration_ms_bucket[5m])))",
          "legendFormat": "{{route}} {{method}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Web Vitals p75 LCP",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.75, sum by (le) (rate(ts_web_vitals_lcp_ms_bucket[10m])))",
          "legendFormat": "LCP p75"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
